$(function() {
	$('.project-name').each(function(){
		if($(this).html()=="域链"){
			$(this).prev().addClass('yulian');
			$(this).attr('href','/project/detail?id=3');
		}
		if($(this).html()=="C链"){
			$(this).prev().addClass('clian');
			$(this).attr('href','/project/detail?id=6');
		}
		if($(this).html()=="Walton"){
			$(this).prev().addClass('walton');
			$(this).attr('href','/project/detail?id=5');
		}
	})
	
})
