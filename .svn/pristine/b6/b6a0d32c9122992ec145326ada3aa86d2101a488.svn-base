}
.page-logreg-wrap .item-error {
    color: #e55600;
    display: none;
}
.page-logreg-wrap .item-p {
    text-align: center;
    margin-bottom: 15px;
}
.page-logreg-wrap .link-btn {
    text-decoration: underline;
    color: #e55600;
}
.page-logreg-wrap .cpy {
    margin-top: 20px;
    font-size: 12px;
    color: #999;
}
.page-logreg-wrap .reg-success-wrap {
    display: none;
}
.page-logreg-wrap .log-verify-wrap {
    display: none;
}
.page-logreg-wrap .forget-send-wrap {
    display: none;
}
.page-logreg-wrap #reVerify {
    color: #669900;
}
.page-logreg-wrap #reVerify:hover {
    text-decoration: underline;
}
.page-logreg-wrap .item-email {
    font-weight: bold;
    margin-bottom: 30px;
}
.location-wrap {
    background-color: #fff;
    padding: 20px 25px;
    border-bottom: 1px solid #dedede;
}
.location-wrap .left {
    float: left;
}
.location-wrap .right {
    float: right;
}
.location-wrap h2 {
    font-size: 24px;
    font-weight: lighter;
    color: #666;
}
.location-wrap ul {
    margin: 5px 0 0 0;
    list-style: none;
}
.location-wrap li {
    float: left;
}
.location-wrap li a {
    color: #666;
    text-decoration: none;
}
.location-wrap ul>li+li:before {
    content: "/\00a0";
    padding: 0 5px;
    color: #ccc;
}
.location-wrap .r-btn {
    display: inline-block;
    color: #fff;
    background-color: #e55600;
    padding: 10px 20px;
    font-size: 16px;
    margin-top: 9px;
}
.location-wrap .r-btn:hover {
    background-color: #ff6a00;
}
.main-wrap {
    padding: 40px 50px;
}
.fc table {}
@media screen and (max-width:768px) {
    .page-index {
        padding: 10px;
    }
    .page-index .price-wrap .item {
        width: 100%;
        margin: 10px 10px;
        float: none;
    }
    .page-index .price-wrap .item .price {
        padding: 15px;
        font-size: 2rem;
        text-align: center;
    }
    .page-index .price-wrap .item .inner {
        margin-right: 0;
    }
    .page-index .price-wrap {
        margin-top: 15px;
        margin-right: 20px;
    }
}
.user-main {
    width: 1300px;
    background: #ffffff;
}
.user-main .user-left-content {
    float: left;
    width: 180px;
}
.user-main .user-right-content {
    float: right;
    width: 1119px;
    min-height: 500px;
    border-left: 1px solid #dedede;
}
.user-main .user-left-content .menu-head {
    color: #333333;
    height: 42px;
    font-size: 14px;
    line-height: 42px;
    font-weight: 600;
    padding-left: 30px;
}
.user-main .user-left-content li {
    height: 42px;
    line-height: 42px;
    list-style-type: none;
    border-left: 3px solid transparent;
}
.user-main .user-left-content a {
    padding-left: 27px;
    font-size: 14px;
    color: #999999;
    text-decoration: none;
}
.user-main .user-left-content a:hover {
    color: #E55600;
}
.user-main .user-left-content .current {
    background: #faf7f2;
    border-left: 3px solid #E55600;
}
.user-main .user-left-content .current a {
    color: #E55600;
}
.layui-laypage .layui-laypage-curr .layui-laypage-em {
    background-color: #e65600;
}
.table-hover>tbody>tr:hover {
    background-color: #f9f9fb !important;
}
.wrapper .form-control[disabled] {
    border: 1px solid #e5e5e5;
    background-color: #f5f5f5;
}
.wrapper .form-control {
    box-shadow: none;
}
.wrapper .form-control:focus {
    box-shadow: none;
    border-color: #e55600;
}
.wrapper .btn {
    border-radius: 0;
}

/* 个人中心表头同意样式 */

.user-center-head .left {
    float: left;
}
.user-center-head img {
    border-radius: 5px;
    margin-right: 12px;
    box-shadow: 0 0 12px 0 rgba(255, 103, 103, 0.5);
}
.user-center-head h2 {
    margin: 5px 0 5px 0;
    font-size: 18px;
    color: #333333;
}
.user-center-head ul {
    margin: 0;
}
.user-center-head li {
    float: left;
}
.user-center-head .left ul>li+li:before {
    content: "/\00a0";
    padding: 0 5px;
    color: #ccc;
}
.user-center-head li a {
    font-size: 14px;
    color: #999999;
    text-decoration: none;
}
.pages {
    text-align: center;
}
.pages .pagination {
    margin: 0;
}
.pagination>li>a,
.pagination>li>span {
    color: #333;
}
.pagination>li:first-child>a,
.pagination>li:first-child>span {
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
}
.pagination>li:last-child>a,
.pagination>li:last-child>span {
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}
.pagination>li>a:hover,
.pagination>li>span:hover,
.pagination>li>a:focus,
.pagination>li>span:focus {
    color: #E55600;
}
.pagination>.active>a,
.pagination>.active>span,
.pagination>.active>a:hover,
.pagination>.active>span:hover,
.pagination>.active>a:focus,
.pagination>.active>span:focus {
    background-color: #E55600;
    border-color: #E55600;
}
.pagination>.disabled>span,
.pagination>.disabled>span:hover,
.pagination>.disabled>span:focus,
.pagination>.disabled>a,
.pagination>.disabled>a:hover,
.pagination>.disabled>a:focus {
    border-color: #ededed;
}
.nav li a:hover {
    color: #e55600;
}
#special{
    margin-left:20px;
}
.navbar-nav{

}
.navbar_border{
    box-sizing: border-box;
    border-bottom: 1px solid #dedede;
}
.navbar-right>li>a:hover, .navbar-right>li>a:focus {
    background-color:inherit;
}
.nav .dropdown{
    display: none;
}
.open>.dropdown-menu {
    display: none;
}
.caret{
    display: none;
}
.user-main {
    width: 100%;
    background: #ffffff;
}
.user-main .user-right-content {
    width: calc(100% - 180px);
}
.phoneshow{
    display: none !important;
}
.collapse .navbar-nav a{
    color:#333;
    padding: 15px 25px;
}
.nav a{
    color:#333;
}
@media screen and (max-width:768px){
    .wrapper{
        overflow: hidden;
    }
    .main{
        padding-top:50px;
    }
    .navbar_border{
        width:100%;
        position: absolute;
        background:transparent;
        z-index:10000;
        border-bottom: 0;
    }
    .navbar-nav {
        margin: 0px 0px !important;
    }
    .collapse .navbar-nav a {
        padding: 10px 15px;
    }
    .navbar-collapse{
        margin-top:0;
        padding-left:0;
        padding-right:0;
        display: block;
        visibility: visible;
        background:#fff;
        position:absolute;
        padding-bottom:10px;
        width:41%;
        left:100%;
        border:1px solid #eee;
        transition:left ease 0.35s;
        overflow: hidden;
    }
    .sliderbar{
        display: none;
    }
    .dropdown-menu li{
        padding-left:25px;
    }
    .box .content {
        padding-top:0;
    }
    .nav .dropdown{
        display: block;
    }
    .table thead>tr>th {
        font-size: 12px !important;
    }
    .table tbody>tr>td {
        font-size: 12px !important;
    }
    .pcshow{
        display: none !important;
    }
    .phoneshow{
        display: block !important;
    }
    .open>.dropdown-menu {
        display: block;
    }
    .caret{
        display: inline-block;
    }
    .dropdown-menu>.active>a, .dropdown-menu>.active>a:hover, .dropdown-menu>.active>a:focus {
        background-color: #e55600;
    }
    .header .navbar-toggle {
        position: relative;
        float: right;
        padding: 9px 10px;
        margin-top: 8px;
        margin-right: 15px;
        margin-bottom: 8px;
        background-color: #e55600;
        background-image: none;
        border: 1px solid orange;
        border-radius: 0px;
    }
    .icon-bar {
        background-color: #fff;
    }
    .account{
        display: none !important;
    }
    .collapse .account{
        display: none ;
    }
    #special{
        margin-left:15px;
    }
    .user-main .user-left-content {
        display: none;
    }
    .user-main .user-right-content {
        float: right;
        width: 100%;
        min-height: 500px;
        border-left: 1px solid transparent;
    }
    .left-in{
        left:55%;
        transition:left ease 0.35s;
    }
}