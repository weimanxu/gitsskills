@CHARSET "UTF-8";
blockquote,
body,
button,
dd,
dl,
dt,
fieldset,
form,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
input,
legend,
li,
ol,
p,
pre,
td,
textarea,
th,
ul {
    margin: 0;
    padding: 0;
}
img,
fieldset,
button,
input {
    border: none;
}
a,
input,
button,
select,
textarea {
    outline: none;
}
a {
    text-decoration: none;
    color: #333;
}
a:focus {
    outline: none;
}
em {
    font-style: normal;
}
dl {
    list-style: none outside none;
}
button {
    outline: none !important;
}
#carousel .carousel-indicators {
    bottom: -13px;
}
.clearfix:after {
    clear: both;
    display: table;
    content: "";
}
.clearfix:before {
    content: "";
    display: table;
}

/* IE 6/7 */

.clearfix {
    zoom: 1;
}
.clear {
    clear: both;
}
.fl {
    float: left;
}
.fr {
    float: right;
}
.hide {
    display: none;
}
.mt15 {
    margin-top: 15px;
}
.mb15 {
    margin-bottom: 15px;
}
.mt20 {
    margin-top: 20px;
}
.mt30 {
    margin-top: 30px !important;
}
.mt40 {
    margin-top: 40px;
}
.mb30 {
    margin-bottom: 30px !important;
}
.box {
    background-color: #fff;
    border: 1px solid #eee;
}
.box .title {
    padding: 10px 15px;
    line-height: 26px;
    border-bottom: 1px solid #eee;
    font-size: 15px;
    font-weight: bold;
    color: #666;
}
.box .content {
    padding: 15px;
}
html {}
body {
    font: 14px/1.5 PingFang SC, Microsoft Yahei, Lantinghei SC, Hiragino Sans GB, Microsoft Sans Serif, WenQuanYi Micro Hei, sans !important;
    color: #333;
}
.body-user {
    background-color: #f9f9f9;
}
.wrap {
    width: 1000px;
    margin: 0 auto;
}
.wrap1200 {
    width: 1200px;
    margin: 0 auto;
}
.btn {
    padding: 9px 15px;
    border: 1px solid transparent;
    box-sizing: border-box;
}
.btn-major {
    background-color: #e55600;
    border-color: #e55600;
    color: #fff;
}
.btn-major:hover {
    background-color: #ff6a00;
    border-color: #ff6a00;
}
.btn-hollow {
    background-color: #fff;
    border-color: #e55600;
    color: #e55600;
}
.btn-hollow:hover {}
.btn-block {
    display: block;
    width: 100%;
    text-align: center;
}
.btn120 {
    width: 120px;
}
.inp {
    border: 1px solid #ccc;
    width: 250px;
    background-color: #fff;
    padding: 8px 12px;
    line-height: 21px;
    box-sizing: border-box;
}
.inp-m {
    line-height: 26px;
    width: 300px;
    font-size: 16px;
}
.inp-l {
    line-height: 41px;
    width: 400px;
    font-size: 18px;
}
.inp300 {
    width: 300px;
}
.inp350 {
    width: 350px;
}
.inp400 {
    width: 400px;
}
.inp-block {
    display: block;
    width: 100%;
}
.inp:focus {
    border-color: #878787;
}
.wrapper {
    max-width: 1300px;
    min-width: 300px;
    margin: 0 auto;
    background-color: #f9f9f9;
    box-shadow: 0px 0px 12px 0px rgba(0, 0, 0, 0.15);
    position: relative;
}
.header {
    box-sizing:border-box;
    line-height: 50px;
    background-color: #fff;
    /*border-bottom: 1px solid #dedede;*/
}
.header .left {
    float: left;
}
.header .right {
    float: right;
}
.header .logo {
    float: left;
    background: url(/public/images/logo.png) 0 0 no-repeat #e55600;
    width: 181px;
    color: #fff;
    font-size: 20px;
    text-align: center;
    text-indent: -9999px;
    height: 50px;
}
.header .nav {
    float: left;
    list-style: none;
    margin-left: 20px;
}
.header .nav li {
    float: left;
}
.header .nav li a {
    padding: 0 25px;
    color: #333;
    display: block;
}
.header .nav li.active a {
    color: #e55600;
}
.header .nav li a:hover {
    color: #e55600;
}
.header .right {
    float: right;
}
.header .right .account {
    float: left;
    margin-right: 20px;
}
.header .right .logout,
.header .right .login {
    float: left;
    margin-right: 20px;
    color: #333;
}
.header .right .account a {
	color: #333;
}
.header .right a:hover {
    color: #e55600;
}
.footer {
    text-align: center;
    padding: 15px 0;
    background-color: #fff;
    color: #999;
    border-top: 1px solid #dedede;
    font-size: 12px;
    line-height: 19px;
}
.sliderbar {
    position: fixed;
    z-index: 100;
    top: 70%;
    width: 60px;
	right: 10px;
}
.sliderbar a {
    text-decoration: none;
}
.sliderbar .item {
    height: 60px;
    position: relative;
    margin-bottom: 10px;
}
.sliderbar .cover {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 60px;
    filter: alpha(opacity=25);
    -moz-opacity: 0.25;
    opacity: 0.25;
    background-color: #000;
}
.sliderbar .inner {
    position: relative;
    z-index: 100;
    padding-top: 7px;
}
.sliderbar .phone .inner {
    padding-top: 15px;
}
.sliderbar .item img {
    display: block;
    width: 30px;
    margin: 0 auto;
}
.sliderbar a:hover .cover {
    background-color: #646464;
    filter: alpha(opacity=100);
    -moz-opacity: 1;
    opacity: 1;
}
.sliderbar .phone .phone-show {
    display: none;
    position: absolute;
    width: 220px;
    height: 60px;
    top: 0;
    left: -235px;
    background-color: #646464;
    line-height: 60px;
    text-align: center;
    color: white;
    font-size: 14px;
}
.sliderbar a:hover .phone .phone-show {
    display: block;
}
.sliderbar .item p {
    text-align: center;
    line-height: 20px;
    font-size: 12px;
    color: #fff;
    margin-top: 3px;
}
.page-index {
    padding: 40px;
}
.page-index .banner-pic-wrap {
    margin-bottom: 20px;
}
.page-index .banner-pic-wrap img {
    display: block;
    width: 100%;
}
.page-index .site-intro {
    text-align: center;
    padding: 25px 0;
}
.page-index .site-intro h2 {
    color: #e55600;
    margin-bottom: 15px;
}
.page-index .price-wrap {
    margin-top: 30px;
    margin-right: -30px;
}
.page-index .price-wrap .item {
    width: 33.3333%;
    float: left;
}
.page-index .price-wrap .item .inner {
    margin-right: 30px;
}
.page-index .price-wrap .item .title .left {
    float: left;
}
.page-index .price-wrap .item .title .right {
    float: right;
    background-color: #669900;
    color: #fff;
    border-radius: 3px;
    line-height: 20px;
    margin-top: 3px;
    padding: 0 10px;
    font-size: 12px;
    font-weight: normal;
}
.page-index .price-wrap .item .price {
    padding: 15px;
    font-size: 30px;
}
.page-index .calendar .inner {
    padding: 15px;
}
.page-index #calendar {
    color: #666;
}
.page-index .fc-agenda .fc-event-time,
.page-index .fc-event,
.fc-event a {
    padding: 4px 6px;
    background-color: #e55600;
    border-color: #e55600;
}
.page-index .fc-unthemed td {
    border-color: #eee;
}
.page-index .fc-nonbusiness {
    background-color: #f9f9f9;
}
.page-logreg {
    background-color: #f9f9f9;
}
.page-logreg-wrap {
    max-width: 360px;
    margin: 0 auto;
    margin-top: 100px;
    padding: 40px;
}
.page-logreg-wrap .reg-wrap {}
.page-logreg-wrap h1 {
    font-size: 36px;
    font-weight: normal;
}
.page-logreg-wrap h4 {
    font-weight: normal;
    color: #666;
}
.page-logreg-wrap form {
    display: block;
    width: 100%;
    margin-top: 30px;
}
.page-logreg-wrap .item {
    margin-bottom: 15px;
}
.page-logreg-wrap .inp.show-error {
