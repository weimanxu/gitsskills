@CHARSET "UTF-8";
.wrapper .modal-dialog {
    width: 400px;
}
.wrapper .modal-header {
    border: 0;
}
.wrapper .modal-content {
    width: 400px;
    box-shadow: 0 5px 18px 0 rgba(0, 0, 0, 0.3);
}
.wrapper .modal-content .form-group {
    margin-bottom: 20px;
}
.wrapper .modal-content .modal-header {
    padding: 15px 15px 20px 15px;
}
.wrapper .modal-content .modal-body {
    padding: 0 15px 15px 15px;
}
.wrapper .modal-title {
    font-size: 18px;
    padding-bottom: 15px;
    margin: 15px 25px 0px;
    text-align: center;
    border-bottom: 1px solid #e5e5e5;
}
.wrapper .modal-content .modal-body input {
    width: 320px;
    height: 44px;
}
.wrapper .modal-content .modal-body form {
    text-align: center;
    text-align: -moz-center;
    text-align: -webkit-center;
}
input,
.wrapper .modal-content {
    border-radius: 0 !important;
}
li {
    list-style-type: none;
}
.user-founds-wrap {
    padding: 40px;
}
.founds-body-nav li {
    float: left;
}
.founds-body {
    margin-top: 40px;
}
.founds-body-nav li a {
    margin: 0 50px -1px 30px;
    font-size: 16px;
    color: #999999;
    display: inline-block;
    padding-bottom: 7px;
    text-decoration: none;
}
.founds-body-nav .active {
    color: #E55600;
    border-bottom: 1px solid #E55600;
}
.founds-body-nav {
    margin-bottom: 30px;
    border-bottom: 1px solid #ededed;
}
.founds-body .notice {
    width: 100%;
    padding: 30px;
    background: #f9f9fb;
    margin-bottom: 40px;
}
.notice h2 {
    margin: 0 0 14px 0;
    font-size: 16px;
    color: #333333;
}
.notice p {
    font-size: 14px;
    color: #666666;
    margin: 0px 0 5px 0;
    letter-spacing: 1px;
}
.notice .set-password {
    padding: 20px 0;
    text-align: center;
}
.notice .set-password a {
    color: #E55600;
}
.founds-body .address-title {
    font-size: 16px;
    color: #333333;
}
.founds-body .address-content {
    font-weight: bold;
    font-size: 18px;
    color: #333333;
}
.founds-body .qrcode {
    margin: 30px 0 40px 0;
}
.founds-body .qrcode img {
    width: 135px;
    height: 135px;
}
.record h2 {
    font-size: 16px;
    color: #333333;
    margin: 10px 0;
}
.founds-body .table {
    border: 1px solid #e5e5e5;
}
.founds-body .table>thead>tr>th {
    color: #666;
    height: 25px;
    line-height: 25px;
    font-weight: normal;
    font-size: 14px;
    background: #f9f9fb;
    text-align: center;
    /* border-bottom: 1px solid #ededed; */
    border-bottom: 0;
}
.founds-body .table>tbody>tr>td {
    height: 25px;
    line-height: 25px;
    font-size: 14px;
    color: #333333;
    text-align: center;
    border: none;
}
.founds-body .table>tbody>tr>td>span {
    font-size: 14px;
    color: #999999;
}
.founds-body .table>tbody>tr>td>a,
.founds-withdrawal a {
    font-size: 14px;
    color: #999999;
}
.founds-body .table>tbody>tr>td>a:hover,
.founds-withdrawal a:hover {
    color: #E55600;
}
.founds-body .table .font-red,
.notice .font-red {
    color: #FF4C4C;
}
.founds-withdrawal {
    margin: -30px 0 40px 0;
}
.founds-withdrawal label {
    margin: 0;
    font-weight: normal;
    color: #333333;
    font-size: 16px;
    width: 100px;
}
.founds-withdrawal input {
    margin-top: 30px;
    display: inline-block;
    width: 440px;
    height: 44px;
    margin-right: 25px;
}
.founds-withdrawal .footnote {
    margin: 10px 0 0 104px;
}
.founds-withdrawal .btn-withdrawal {
    width: 140px;
    height: 44px;
    margin: 40px 0 0 104px;
    font-size: 14px;
    color: #FFFFFF;
    background-color: #e65600;
}
.founds-withdrawal .btn-withdrawal:hover,
#addressForm .btn-address:hover {
    color: #FFFFFF;
    background-color: #fd5f00;
}
#addressForm .btn-address {
    width: 320px;
    height: 44px;
    color: #FFFFFF;
    margin: 10px 0 25px 0;
    background-color: #e65600;
}
.founds-withdrawal .btn-withdrawal:focus,
#addressForm .btn-address:focus {
    color: #FFFFFF;
    background-color: #e65600;
}
.founds-withdrawal .new-add {
    margin-top: 30px;
    font-size: 14px;
    color: #E55600;
    display: inline-block;
}
.founds-withdrawal .new-tip {
    margin-left: 255px;
    font-size: 14px;
    color: #FF4C4C;
}
@media screen and (max-width:768px){
    .founds-withdrawal .new-add {
        margin-left: -25px;
    }
    .founds-withdrawal .new-tip {
        margin-left: 16px;
    }
    .user-founds-wrap {
        padding: 15px;
    }
    .founds-body .address-content {
        font-size: 12px;
    }
    .founds-body .qrcode {
       text-align: center;
    }
    .phone-hidden{
        display: none;
    }
    .founds-withdrawal input {
        margin-top: 5px;
        width: 80%;
        height: 35px;
        margin-right: 10px;
        margin-bottom:5px;
    }
    .founds-withdrawal .footnote {
        margin: 0;
        margin-bottom:15px;
        color:#999;
    }
    #withdrawalForm{
        padding-top:20px;
    }
    .founds-withdrawal .btn-withdrawal {
        margin: 10px 0 0 70px;
        font-size: 14px;
    }
    .w25{
        width:25%;
    }
    .pagination{
        font-size:12px;
    }
    .wrapper .modal-dialog {
        width: 100%;
    }
    .wrapper .modal-content {
        width: 90%;
    }
    .wrapper .modal-content .modal-body input {
        width: 80%;
    }
    #addressForm .btn-address {
        width: 85%;
    }
    .modal-dialog {
        margin: 10% 5%;
    }
}